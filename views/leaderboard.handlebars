<h1>Leaderboard</h1>
<div class="column">
    <div class="card leader">
        <div class="card-content ">
            <div class="content">
                <div>User | Score</div>
                <ol>
                    {{#each leaderArray}}
                    <li class="">{{this.[0]}} - {{this.[1]}}</li>

                    {{/each}}
                </ol>
            </div>
        </div>
    </div>
</div>